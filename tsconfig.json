{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "allowJs": true,
    "outDir": "out",
    "baseUrl": "src",
    "paths": {
      "image-target/*": [
        "./src/image-target/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ]
    },
  },
  "include": ["src"]
}
